{
    "name": "cambis/silverstripe-classnames",
    "description": "Classnames package for Silverstripe.",
    "keywords": [
        "Silverstripe"
    ],
    "author": "Cameron Bryers",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "newridetech/php-classnames": "^1.2",
        "silverstripe/framework": "^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Cambis\\Classnames\\": "src/",
            "Cambis\\Classnames\\Tests\\": "tests/php/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    }
}
